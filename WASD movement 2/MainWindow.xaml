<Window x:Class="GameW.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameW"
        mc:Ignorable="d"
        Title="GAME!" Height="500" Width="500">

    <Canvas Name="cnvs" Focusable="true"  Background="LightBlue" Margin="0,0,0,0" KeyDown="cnvs_KeyDown" KeyUp="cnvs_KeyUp"  MouseLeftButtonDown="cnvs_MouseLeftButtonDown">
        <Image Name="StartImage" Height="50"  Source="images/start.png" Canvas.Left="-156" Canvas.Top="-474" Width="100" MouseDown="image_MouseDown" RenderTransformOrigin="0.31,0.56" MouseUp="StartImage_MouseUp"/>
        <Image Name="player1" Height="80" Width="80" Source="images/Sprite-0002.png" Canvas.Left="26" Canvas.Top="-474" ></Image>
        <Image Name="gun1" Height="10" Width="10" Source="images/gun.png" Canvas.Left="-12" Canvas.Top="-474"></Image>
        <Image Name="bullet1" Height="3" Width="4" Source="images/bullet1.png" Canvas.Left="4" Canvas.Top="-474"></Image>
        <TextBox x:Name="textBox" Height="23" Canvas.Left="188" TextWrapping="Wrap" Text="TextBox" Canvas.Top="10" Width="120"/>
    </Canvas>
</Window>
